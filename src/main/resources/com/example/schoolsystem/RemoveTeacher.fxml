<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.schoolsystem.PrincipleDataController">
    <children>
        <Pane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #000326;">
            <children>
                <Label layoutX="12.0" layoutY="112.0" prefHeight="38.0" prefWidth="137.0" text="Teachers Id:-" textFill="#f2a532">
                    <font>
                        <Font name="Ebrima Bold" size="14.0" />
                    </font></Label>
                <Button fx:id="PrincipleTeacherRemove" layoutX="196.0" layoutY="330.0" mnemonicParsing="false" onAction="#onRemoveATeacher" onMouseEntered="#onPrincipleTeacherRemoveMouse" onMouseExited="#outPrincipleTeacherRemoveMouse" prefHeight="49.0" prefWidth="205.0" style="-fx-background-color: #f2a532;" text="Remove " textFill="#000326">
                    <font>
                        <Font name="Ebrima Bold" size="14.0" />
                    </font>
               <graphic>
                  <ImageView fitHeight="23.0" fitWidth="25.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@remove.png" />
                     </image>
                  </ImageView>
               </graphic></Button>
                <TextField fx:id="TeId" layoutX="180.0" layoutY="112.0" prefHeight="38.0" prefWidth="342.0" />
                <HBox prefHeight="0.0" prefWidth="600.0">
                    <children>
                        <Label prefHeight="63.0" prefWidth="555.0" text="Remove Teacher" textFill="#f2a532">
                            <font>
                                <Font name="Ebrima Bold" size="24.0" />
                            </font>
                            <padding>
                                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                            </padding>
                        </Label>
                        <ImageView fitHeight="103.0" fitWidth="75.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@new.jpeg" />
                     </image></ImageView>
                    </children>
                </HBox>
              <Button fx:id="backrs" layoutX="545.0" layoutY="351.0" mnemonicParsing="false" onAction="#techBackspace" onMouseEntered="#onbackrsMouse" onMouseExited="#outbackrsMouse" prefHeight="49.0" prefWidth="56.0" style="-fx-background-color: #000326;">
                  <graphic>
                      <ImageView fitHeight="33.0" fitWidth="48.0" pickOnBounds="true" preserveRatio="true">
                          <image>
                              <Image url="@backspace.png" />
                          </image>
                      </ImageView>
                  </graphic>
              </Button>
            </children></Pane>
    </children>

</AnchorPane>
