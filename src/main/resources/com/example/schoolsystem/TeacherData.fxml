<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.schoolsystem.TeacherDataController">
    <children>
        <Pane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #000326;">
            <children>
                <HBox prefHeight="118.0" prefWidth="600.0">
                    <children>
                        <Label prefHeight="118.0" prefWidth="522.0" text="Teacher Portal" textFill="#f2a532">
                            <font>
                                <Font name="Ebrima Bold" size="24.0" />
                            </font>
                     <padding>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </padding>
                        </Label>
                        <ImageView fitHeight="118.0" fitWidth="103.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@new.jpeg" />
                     </image></ImageView>
                    </children>
                </HBox>
                <Button fx:id="TeacherAddGrades" layoutX="300.0" layoutY="136.0" mnemonicParsing="false" onAction="#onAddGrades" onMouseEntered="#onTeacherAddGradesMouse" onMouseExited="#outTeacherAddGradesMouse" prefHeight="103.0" prefWidth="185.0" style="-fx-background-color: #f2a532;" text="Add Grades" textFill="#000326">
                    <font>
                        <Font name="Ebrima Bold" size="14.0" />
                    </font>
               <graphic>
                  <ImageView fitHeight="45.0" fitWidth="41.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@score.png" />
                     </image>
                  </ImageView>
               </graphic></Button>
                <Button fx:id="TeacherProfile" layoutX="80.0" layoutY="136.0" mnemonicParsing="false" onAction="#onTeacherProfile" onMouseEntered="#onTeacherProfileMouse" onMouseExited="#outTeacherProfileMouse" prefHeight="103.0" prefWidth="185.0" style="-fx-background-color: #f2a532;" text="Profile" textFill="#000326">
                    <font>
                        <Font name="Ebrima Bold" size="14.0" />
                    </font>
               <graphic>
                  <ImageView fitHeight="40.0" fitWidth="27.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@profile.png" />
                     </image>
                  </ImageView>
               </graphic>
                </Button>
                <Button fx:id="TeacherRequest" layoutX="80.0" layoutY="258.0" mnemonicParsing="false" onAction="#onRequests" onMouseEntered="#onTeacherRequestMouse" onMouseExited="#outTeacherRequestMouse" prefHeight="103.0" prefWidth="185.0" style="-fx-background-color: #f2a532;" text="Request" textFill="#000326">
                    <font>
                        <Font name="Ebrima Bold" size="14.0" />
                    </font>
               <graphic>
                  <ImageView fitHeight="24.0" fitWidth="32.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@edit.png" />
                     </image>
                  </ImageView>
               </graphic>
                </Button>
                <Button fx:id="TeacherLogout" layoutX="300.0" layoutY="258.0" mnemonicParsing="false" onAction="#onLogout" onMouseEntered="#onTeacherLogoutMouse" onMouseExited="#outTeacherLogoutMouse" prefHeight="103.0" prefWidth="185.0" style="-fx-background-color: #f2a532;" text="Logout" textFill="#000326">
                    <font>
                        <Font name="Ebrima Bold" size="14.0" />
                    </font>
               <graphic>
                  <ImageView fitHeight="34.0" fitWidth="31.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@logout.png" />
                     </image>
                  </ImageView>
               </graphic>
                </Button>
            </children>
        </Pane>
    </children>

</AnchorPane>
