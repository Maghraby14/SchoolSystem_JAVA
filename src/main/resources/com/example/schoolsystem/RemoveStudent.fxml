<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.schoolsystem.PrincipleDataController">
    <children>
        <Pane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #000326;">
            <children>
                <Label layoutX="12.0" layoutY="112.0" prefHeight="38.0" prefWidth="137.0" text="Students Id:-" textFill="#f2a532">
                    <font>
                        <Font name="Ebrima Bold" size="14.0" />
                    </font></Label>
                <Button fx:id="PrincipleStudentRemove" layoutX="198.0" layoutY="316.0" mnemonicParsing="false" onAction="#onRemoveAStudent" onMouseEntered="#onPrincipleStudentRemoveMouse" onMouseExited="#outPrincipleStudentRemoveMouse" prefHeight="49.0" prefWidth="205.0" style="-fx-background-color: #f2a532;" text="Remove" textFill="#000326">
                    <font>
                        <Font name="Ebrima Bold" size="14.0" />
                    </font>
               <graphic>
                  <ImageView fitHeight="23.0" fitWidth="23.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@remove.png" />
                     </image>
                  </ImageView>
               </graphic></Button>
                <TextField fx:id="ReSId" layoutX="180.0" layoutY="112.0" prefHeight="38.0" prefWidth="342.0" />
                <HBox prefHeight="67.0" prefWidth="600.0">
                    <children>
                        <Label prefHeight="66.0" prefWidth="541.0" text="Remove Student" textFill="#f2a532">
                            <font>
                                <Font name="Ebrima Bold" size="24.0" />
                            </font>
                            <padding>
                                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                            </padding>
                        </Label>
                        <ImageView fitHeight="90.0" fitWidth="72.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@new.jpeg" />
                     </image></ImageView>

                    </children>
                </HBox>
              <Button fx:id="backrt" layoutX="522.0" layoutY="339.0" mnemonicParsing="false" onAction="#stuBackspace" onMouseEntered="#onbackrtMouse" onMouseExited="#outbackrtMouse" prefHeight="53.0" prefWidth="66.0" style="-fx-background-color: #000326;">
               <graphic>
                      <ImageView fitHeight="43.0" fitWidth="41.0" pickOnBounds="true" preserveRatio="true">
                          <image>
                              <Image url="@backspace.png" />
                          </image>
                      </ImageView>
               </graphic>
            </Button>
            </children></Pane>
    </children>
</AnchorPane>
