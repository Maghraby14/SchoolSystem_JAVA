                  <Font name="Ebrima Bold" size="14.0" />
               </font>
               <graphic>
                  <ImageView fitHeight="29.0" fitWidth="35.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@remove.png" />
                     </image>
                  </ImageView>
               </graphic></Button>
            <Button fx:id="AddnewTeacher" layoutX="64.0" layoutY="131.0" mnemonicParsing="false" onAction="#onAddTeacher" onMouseEntered="#onAddnewTeacherMouse" onMouseExited="#outAddnewTeacherMouse" prefHeight="118.0" prefWidth="185.0" style="-fx-background-color: #f2a532;" text="Add Teacher" textFill="#000326">
