               </font>
               <graphic>
                  <ImageView fitHeight="36.0" fitWidth="32.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@add.png" />
                     </image>
                  </ImageView>
               </graphic></Button>
            <Button fx:id="backtc" layoutX="527.0" layoutY="333.0" mnemonicParsing="false" onAction="#NorBackspace" onMouseEntered="#onbacktcMouse" onMouseExited="#outbacktcMouse" prefHeight="53.0" prefWidth="66.0" style="-fx-background-color: #000326;">
               <graphic>
                  <ImageView fitHeight="43.0" fitWidth="41.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@backspace.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
